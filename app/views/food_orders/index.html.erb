<p id="notice"><%= notice %></p>

<h1>Listing Food Orders</h1>

<table>
  <thead>
    <tr>
      <th>Food</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @food_orders.each do |food_order| %>
      <tr>
        <td><%= food_order.food_id %></td>
        <td><%= link_to 'Show', food_order %></td>
        <td><%= link_to 'Edit', edit_food_order_path(food_order) %></td>
        <td><%= link_to 'Destroy', food_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food order', new_food_order_path %>
