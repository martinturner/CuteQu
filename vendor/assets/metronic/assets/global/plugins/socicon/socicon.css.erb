/*
Social icons font: <a href="http://www.socicon.com" target="_blank" alt="Free social icons font" title="the social icons font">socicon</a>
*/

@font-face {
    font-family: 'socicon';
    src: url(<%= asset_path 'assets/global/plugins/socicon/font/socicon.eot' %>);
    src: url(<%= asset_path 'assets/global/plugins/socicon/font/socicon.eot?#iefix' %>) format('embedded-opentype'),
         url(<%= asset_path 'assets/global/plugins/socicon/font/socicon.woff' %>) format('woff'),
         url(<%= asset_path 'assets/global/plugins/socicon/font/socicon.woff2' %>) format('woff2'),
         url(<%= asset_path 'assets/global/plugins/socicon/font/socicon.ttf' %>) format('truetype'),
         url(<%= asset_path 'assets/global/plugins/socicon/font/socicon.svg#sociconregular' %>) format('svg');
    font-weight: normal;
    font-style: normal;
    text-transform: initial;
}